<div class="page ml-2"><p> likes </p></div>
<p> your latest intake of news </p>
<div class="cards">
  <% @likes.each do |like| %>
    <div class="card">
      <div class="article_image">
        <%= image_tag(like.article.image_url) %>
      </div>
      <div class="overlay"></div>
      <div class="read-time">
        <p><%= like.article.read_mins  %>mins</p>
      </div>
      <div class="headline">
        <%= link_to "#{like.article.title}", article_show_path(like.article)%>
      </div>
    </div>
  <%end%>
</div>
